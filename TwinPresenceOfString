
#include<bits/stdc++.h>
using namespace std;
int main() {
	//code
	int t;
	cin>>t;
	
	while(t>0)
	{    map<string,int> m;
	    int n;
	    cin>>n;
	    
	    string str;
	    
	    for(int i=0;i<n;i++)
	    {
	        cin>>str;
	        m[str]++;
	    }
	    int cnt=0;
	    for(map<string,int>::iterator i=m.begin();i!=m.end();i++)
	    {
	        if(i->second==2)
	        {
	            cnt++;
	        }
	    }
	    
	    cout<<cnt<<endl;
	    
	    t--;
	}
	return 0;
}
